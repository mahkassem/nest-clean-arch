{
  "name": "nest-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "node dist/infrastructure/presistance/seeders/seeder",
    "seed:refresh": "node dist/infrastructure/presistance/seeders/seeder --refresh",
    "update": "npx ncu -u -f \"/nestjs*/\" && npm install"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "^0.1.7",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.9",
    "@slynova/flydrive": "^1.0.3",
    "@slynova/flydrive-s3": "^1.0.3",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "joi": "^17.8.3",
    "mysql2": "^3.1.2",
    "nestjs-i18n": "^10.2.6",
    "npm-check-updates": "^16.7.4",
    "passport-jwt": "^4.0.1",
    "pluralize": "^8.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "sharp": "^0.31.3",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/sequelize": "^4.28.14",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^6.0.1",
    "i": "^0.3.7",
    "jest": "29.3.1",
    "nestjs-seeder": "^0.3.0",
    "npm": "^9.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
